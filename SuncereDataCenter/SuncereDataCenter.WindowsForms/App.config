<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <sectionGroup name="common">
      <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="CityHourlyAirQualitySyncCronExpression" value="0 4/5 * * * ?" />
    <add key="CityDailyAirQualitySyncCronExpression" value="0 30 * * * ?" />
    <add key="CityMonthlyAirQualitySyncCronExpression" value="0 32 * * * ?" />
    <add key="CityQuarterlyAirQualitySyncCronExpression" value="0 32 * * * ?" />
    <add key="CityYearlyAirQualitySyncCronExpression" value="0 32 * * * ?" />
    <add key="ServiceName" value="SuncereDataCenterService" />
    <add key="DisplayName" value="SuncereDataCenterService" />
    <add key="Description" value="广东旭诚科技有限公司数据中心后台服务" />
  </appSettings>
  <connectionStrings>
    <add name="SuncereDataCenterModel" connectionString="metadata=res://*/SuncereDataCenterModel.csdl|res://*/SuncereDataCenterModel.ssdl|res://*/SuncereDataCenterModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.;initial catalog=SuncereDataCenter;user id=sa;password=123456;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
    <add name="EnvPublishStdEntities" connectionString="metadata=res://*/EnvPublishStdModel.csdl|res://*/EnvPublishStdModel.ssdl|res://*/EnvPublishStdModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.;initial catalog=EnvPublishStd;user id=sa;password=123456;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <common>
    <logging>
      <factoryAdapter type="Common.Logging.Log4Net.Log4NetLoggerFactoryAdapter, Common.Logging.Log4net1215">
        <arg key="configType" value="INLINE" />
      </factoryAdapter>
    </logging>
  </common>
  <log4net>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="Log\\" />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <datePattern value="yyyyMMdd'.txt'" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="10MB" />
      <staticLogFileName value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%n【时间】%date   【记录器】%logger %n【级别】%-5level   【线程】%thread %n【位置】%location %n【描述】%message %newline" />
      </layout>
    </appender>
    <root>
      <level value="INFO" />
      <appender-ref ref="RollingFileAppender" />
    </root>
  </log4net>
</configuration>